{
  "name": "welab",
  "version": "0.0.1",
  "description": "",
  "author": "wemanity",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:jest": "jest --config ./test/jest-e2e.json",
    "test:domain:mission": "cucumber-js -p mission",
    "test:e2e:mission": "cucumber-js -p missionE2E",
    "test:domain:cooperator": "cucumber-js -p cooperator",
    "test:e2e:cooperator": "cucumber-js -p cooperatorE2E",
    "test:domain": "cucumber-js -p mission -p cooperator",
    "db:populate:missions": "./src/utils/scripts/missions.sh",
    "db:clear:missions" : "./src/utils/scripts/clearMissionTable.sh",
    "db:populate:cooperators": "./src/utils/scripts/cooperators.sh",
    "db:clear:cooperators" : "./src/utils/scripts/clearCooperatorTable.sh"
  },
  "dependencies": {
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "@nestjs/cli": "^8.2.4",
    "@nestjs/common": "^8.4.1",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "^8.0.3",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "chai": "^4.3.6",
    "dotenv": "^16.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.43"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.2",
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "better-sqlite3": "^7.5.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.6.3"
  }
}
